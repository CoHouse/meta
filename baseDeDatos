/*=============================================>>>>>
= Estructura de tablas necesarias =
===============================================>>>>>*/

categories
  name: String,
  modifier: String,
  type: String

posts
  title: String,
  subtitle: String,
  image: String,
  date: String,
  author: String,
  visibleLevel: String,
  category: String,
  text: String

newsletters
  email : String,
  terms : String


/*= End of Fin de las estructuras =*/


